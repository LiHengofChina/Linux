  echo "ABCDKDKDKSKDKDKSKDKS" | base64 -i			//编码 base64
	echo ASDFSGSADGDFGSDFGDSFGDSFGDSF | base64 -d	//解码 base64	
  
  //======================================= java代码解码
  
  
  new String(new BASE64Decoder().decodeBuffer(config.getProperty("hi")))
  
  
